@model Prueba2.Data.Entities.Country

@{
    ViewData["Title"] = "Delete";
}

<h1>Borrar</h1>

<h3>¿Estás seguro de eliminar este registro?</h3>
<div>
    <partial name="_CountryDetails" />
    <form asp-action="Delete">
        <input type="hidden" asp-for="Id" />
        <input type="submit" value="Borrar" class="btn btn-outline-danger" />
        <a asp-action="Index" class="btn btn-outline-secondary">Regresar</a>
    </form>
</div>
